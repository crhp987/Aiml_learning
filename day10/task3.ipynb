    "import pandas as pd\n",
    "\n",
    "df = pd.DataFrame({\n",
    "    \"Location\": [\" New York\", \"new york\", \"NEW YORK \", \" New York \"]\n",
    "})\n",
    "\n",
    "df[\"Location\"] = df[\"Location\"].str.strip().str.title()\n",
    "\n",
    "\n",
    "print(\"Unique locations:\", df[\"Location\"].unique())"
